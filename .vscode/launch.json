    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run example app with API key access token (debug)",
            "type": "dart",
            "request": "launch",
            "templateFor": "example/lib/",
            "toolArgs": [
                "--dart-define-from-file=../../flutter/arcgis_maps/test/env.json"
            ],
        },
       
        {
            "name": "Compass Needle Preview (Android)",
            "type": "dart",
            "request": "launch",
            "program": "example/lib/main_compass_preview.dart"
        },
        {
            "name": "Run example app with API key access token (profile)",
            "type": "dart",
            "flutterMode": "profile",
            "request": "launch",
            "templateFor": "example/lib/",
            "toolArgs": [
                "--dart-define-from-file=env.json"
            ],
        },
        {
            "name": "Run example app with API key access token (release)",
            "type": "dart",
            "flutterMode": "release",
            "request": "launch",
            "templateFor": "example/lib/",
            "toolArgs": [
                "--dart-define-from-file=env.json"
            ],
        },
        {
            "name": "Run widgetbook with API key access token (release)",
            "type": "dart",
            "flutterMode": "release",
            "request": "launch",
            "program": "example/lib/widget_book/widgetbook.dart",
            "toolArgs": [
               "--dart-define-from-file=../../flutter/arcgis_maps/test/env.json"
            ]
        },
    ]
}